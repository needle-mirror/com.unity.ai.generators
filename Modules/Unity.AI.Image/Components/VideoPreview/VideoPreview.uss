.image-scale-to-fit {
    -unity-background-scale-mode: scale-to-fit;
}

.image-scale-initial {
    -unity-background-scale-mode: initial;
    background-position: center;
    background-repeat: no-repeat;
    background-size: auto;
}

.spinner {
    background-image: url("/Packages/com.unity.ai.toolkit/Modules/Accounts/Icons/Spinner.png");
}
