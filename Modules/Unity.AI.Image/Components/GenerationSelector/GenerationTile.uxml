<UXML xmlns="UnityEngine.UIElements" class="generation-tile border-2px flex-grow">
    <Style src="./GenerationTile.uss"/>
    <VisualElement name="image" picking-mode="Ignore" class="flex-grow bg-medium rounded-4px image-scale-to-fit ">
        <VisualElement name="border" picking-mode="Ignore" class="flex-grow border-2px border-selectable rounded-4px" />
        <Label name="label" text="" picking-mode="Ignore" class="hide absolute-centered-label font-size-11px font-bold bg-medium-light" />
        <VisualElement name="progress" picking-mode="Ignore" class="flex-grow-100pct image-scale-to-fit" />
        <Label name="type" text="" picking-mode="Ignore" class="hide badge-upper-left-label font-size-14px font-bold" />
    </VisualElement>
</UXML>
