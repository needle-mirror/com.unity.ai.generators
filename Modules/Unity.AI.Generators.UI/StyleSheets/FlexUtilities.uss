/* Color */
.color-dark-gray {
    color: var(--unity-colors-window-text);
}

.color-tabs {
    color: var(--unity-colors-tab-text);
}

.color-helpbox {
    color: var(--unity-colors-helpbox-text);
}

/* Background Colors */
.color-tab-background {
    background-color: var(--unity-colors-tab-background-checked);
}

.bg-medium-light {
    background-color: var(--unity-colors-scrollbar_thumb-border);
}

.bg-medium {
    background-color: var(--unity-colors-object_field-background);
}

.bg-transparent {
    background-color: transparent;
}

.bg-light-transparent-50 {
    background-color: rgba(60, 60, 60, 0.5);
}

.bg-dark {
    background-color: var(--unity-colors-highlight-background-inactive);
}

.bg-very-dark {
    background-color: var(--unity-colors-app_toolbar_button-border_accent);
}

/* Border Radius */
.rounded-2px {
    border-radius: 2px;
    overflow: hidden;
}

.rounded-4px {
    border-radius: 4px;
    overflow: hidden;
}

.rounded-6px {
    border-radius: 6px;
    overflow: hidden;
}

.rounded-8px {
    border-radius: 8px;
    overflow: hidden;
}

.rounded-10px {
    border-radius: 10px;
    overflow: hidden;
}

.rounded-12px {
    border-radius: 12px;
    overflow: hidden;
}

.rounded {
    border-radius: 50%;
    overflow: hidden;
}

/* Border Width */
.border-none {
    border-width: 0;
}

.border-1px {
    border-width: 1px;
}

.border-2px {
    border-width: 2px;
}

.border-top-1px {
    border-top-width: 1px;
}

.border-bottom-1px {
    border-bottom-width: 1px;
}

.border-color-dark border-bottom-1px {
    border-bottom-width: 1px;
}

.border-left-1px {
    border-left-width: 1px;
}

/* Border Color */
.border-color-dark {
    border-color: var(--unity-colors-inspector_titlebar-border);
}

.border-color-medium-gray {
    border-color: var(--unity-colors-button-border);
}

/* Text Overflow */
.text-ellipsis {
    flex-wrap: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.text-wrap {
    flex-wrap: wrap;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
}

/* Overflow */
.overflow-hidden {
    overflow: hidden;
}

/* Positioning */
.relative {
    position: relative;
}

.absolute {
    position: absolute;
}

.absolute-centered-label {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 0;
    height: 0;
    transform-origin: center;
    -unity-text-align: middle-center;
}

.absolute-upper-left-label {
    position: absolute;
    left: 0;
    top: 0;
    width: 0;
    height: 0;
    transform-origin: center;
    -unity-text-align: upper-left;
}

/* Font Styles */
.font-bold {
    -unity-font-style: bold;
}

.white-space-normal {
    white-space: normal;
}

.font-size-11px {
    font-size: 11px;
}

.font-size-14px {
    font-size: 14px;
}

.font-size-20px {
    font-size: 20px;
}

/* Text Alignment */
.text-align-middle-center {
    -unity-text-align: middle-center;
}


/* Ping */
.aitk-pingable {
    transition-delay: 0s;
    transition-duration: 500ms;
    transition-property: background-color;
    transition-timing-function: ease;
    scale: 1;
}

.aitk-ping {
    background-color: var(--unity-colors-button-background-pressed);
}
