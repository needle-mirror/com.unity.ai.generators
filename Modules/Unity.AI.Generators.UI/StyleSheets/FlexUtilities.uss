/* Flex Container Classes */

:root {
    --aitk-colors-selected: #3983BC;
}

/* Display Flex */
.flex {
    display: flex;
}

.hide {
    display: none;
}

/* Flex Position */
.flex-absolute {
    position: absolute;
}

.flex-bottom {
    position: absolute;
    bottom: 0;
}

.flex-left {
    position: absolute;
    left: 0;
}

.flex-right {
    position: absolute;
    right: 0;
}

.right-4px {
    position: absolute;
    right: 4px;
}

.right-12px {
    position: absolute;
    right: 12px;
}

.top-12px {
    position: absolute;
    top: 12px;
}

/* Flex Direction */
.flex-row {
    flex-direction: row;
}

.flex-row-reverse {
    flex-direction: row-reverse;
}

.flex-column {
    flex-direction: column;
}

.flex-column-reverse {
    flex-direction: column-reverse;
}

/* Flex Wrapping */
.flex-wrap {
    flex-wrap: wrap;
}

.flex-nowrap {
    flex-wrap: nowrap;
}

/* Flex Item Classes */

/* Flex Grow */
.flex-grow {
    flex-grow: 1;
}

.flex-no-grow {
    flex-grow: 0;
}

.flex-grow-100pct {
    flex-grow: 1;
    width: 100%;
    height: 100%;
}

/* Flex Shrink */
.flex-shrink {
    flex-shrink: 1;
}

.flex-no-shrink {
    flex-shrink: 0;
}

/* Flex Basis */
.flex-basis-200px {
    flex-basis: 200px;
}

.flex-basis-100px {
    flex-basis: 100px;
}

.flex-basis-50pct {
    flex-basis: 50%;
}

/* Height */
.height-16px {
    height: 16px;
}

.height-20px {
    height: 20px;
}

.height-24px {
    height: 24px;
}

.height-32px {
    height: 32px;
}

.height-128px {
    height: 128px;
}

.height-168px {
    height: 168px;
}

/* Min Height */
.min-height-16px {
    min-height: 16px;
}

/* Min Width */
.min-width-16px {
    min-width: 16px;
}

.min-width-50px {
    min-width: 50px;
}

/* Width */
.width-16px {
    width: 16px;
}

.width-20px {
    width: 20px;
}

.width-24px {
    width: 24px;
}

.width-72px {
    width: 72px;
}

.width-100pct {
    width: 100%;
}

.width-180px {
    width: 180px;
}

/* Size */
