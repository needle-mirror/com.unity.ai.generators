.unity-text-element {
    padding-left: 20px;
    height: 20px;
}

.unity-text-element .unity-image {
    position: absolute;
    left: 2px;
    top: 4px;
    width: 12px;
    height: 12px;
    background-image: resource("Packages/com.unity.ai.generators/modules/Unity.AI.Generators.UI/Icons/MagnifyingGlass.png");
    -unity-background-image-tint-color: #a0a0a0;
}
